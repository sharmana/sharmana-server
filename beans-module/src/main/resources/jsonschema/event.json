{
    "type": "object",
    "$schema": "http://json-schema.org/draft-03/schema#",
    "required": false,
    "properties": {
        "_id": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "currency": {
            "type": "string"
        },
        "created": {
            "javaType": "java.lang.Long",
            "type": "object"
        },
        "emails": {
            "type": "object",
            "javaType": "java.util.List<java.lang.String>"
        },
        "transactions": {
            "type": "object",
            "javaType": "java.util.List<ru.sharmana.beans.Transaction>"
        },
        "status": {
            "type" : "enum",
            "enum": [
                "open",
                "closed"
            ]
        }
    }
}